<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<div xmlns:field="urn:jsptagdir:/WEB-INF/tags/form/fields" xmlns:form="urn:jsptagdir:/WEB-INF/tags/form" xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0">
    <jsp:directive.page contentType="text/html;charset=UTF-8"/>
    <jsp:output omit-xml-declaration="yes"/>
    <field:input field="pin" id="c_ua_org_tumakha_spdtool_User_pin" validationMessageCode="field_invalid_integer"/>
    <field:checkbox field="active" id="c_ua_org_tumakha_spdtool_User_active"/>
    <field:input field="firstname" id="c_ua_org_tumakha_spdtool_User_firstname"/>
    <field:input field="firstnameEn" id="c_ua_org_tumakha_spdtool_User_firstnameEn"/>
    <field:input field="lastname" id="c_ua_org_tumakha_spdtool_User_lastname"/>
    <field:input field="lastnameEn" id="c_ua_org_tumakha_spdtool_User_lastnameEn"/>
    <field:input field="middlename" id="c_ua_org_tumakha_spdtool_User_middlename"/>
    <field:input field="middlenameEn" id="c_ua_org_tumakha_spdtool_User_middlenameEn"/>
    <field:select field="regDocumentType" id="c_ua_org_tumakha_spdtool_User_regDocumentType" enumClass="ua.org.tumakha.spdtool.enums.RegDocumentType"/>
    <field:datetime dateTimePattern="${user_regdate_date_format}" field="regDate" id="c_ua_org_tumakha_spdtool_User_regDate"/>
    <field:input field="regNumber" id="c_ua_org_tumakha_spdtool_User_regNumber"/>
    <field:input field="email" id="c_ua_org_tumakha_spdtool_User_email" validationMessageCode="field_invalid_email"/>
    <field:input field="phone" id="c_ua_org_tumakha_spdtool_User_phone" validationMessageCode="field_invalid_integer"/>
    <field:textarea field="regDpi" id="c_ua_org_tumakha_spdtool_User_regDpi"/>
    <field:datetime dateTimePattern="${user_regdatedpi_date_format}" field="regDateDpi" id="c_ua_org_tumakha_spdtool_User_regDateDpi"/>
    <field:input field="regNumberDpi" id="c_ua_org_tumakha_spdtool_User_regNumberDpi" validationMessageCode="field_invalid_integer"/>
    <field:select field="groups" id="c_ua_org_tumakha_spdtool_User_userGroups" itemValue="groupId" items="${groups}" multiple="true" path="/groups"/>
    <field:select field="kveds" id="c_ua_org_tumakha_spdtool_User_kveds" itemValue="kvedId" items="${kveds}" multiple="true" path="/kveds"/>
    <field:textarea field="serviceType.name" id="c_ua_org_tumakha_spdtool_servicetype_name"/>
    <field:textarea field="serviceType.nameEn" id="c_ua_org_tumakha_spdtool_servicetype_nameen"/>
</div>
